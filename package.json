{
  "name": "retext-keywords",
  "version": "1.0.1",
  "description": "Keyword extraction with Retext",
  "license": "MIT",
  "keywords": [
    "keyword",
    "phrase",
    "terminology",
    "term",
    "extraction",
    "retext"
  ],
  "dependencies": {
    "nlcst-to-string": "^1.0.0",
    "retext-pos": "^1.0.0",
    "stemmer": "^0.1.4",
    "unist-util-visit": "^1.0.0"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/retext-keywords.git"
  },
  "bugs": "https://github.com/wooorm/retext-keywords/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
    "Vladimir Starkov <iamstarkov@gmail.com>"
  ],
  "devDependencies": {
    "browserify": "^13.0.1",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.1",
    "istanbul": "^0.4.0",
    "jscs": "^3.0.0",
    "jscs-jsdoc": "^2.0.0",
    "remark": "^4.0.0",
    "remark-comment-config": "^3.0.0",
    "remark-github": "^4.0.1",
    "remark-lint": "^4.0.0",
    "remark-validate-links": "^3.0.0",
    "mocha": "^2.0.0",
    "retext": "^2.0.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --ignore-missing --no-builtins --standalone retextKeywords > retext-keywords.js",
    "build-mangle": "esmangle retext-keywords.js > retext-keywords.min.js",
    "build": "npm run build-bundle && npm run build-md && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "mocha --check-leaks test.js",
    "test-coverage": "istanbul cover _mocha -- test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
